# Apify for Google Maps scraping
APIFY_TOKEN=your_apify_token_here

# Azure OpenAI for AI message generation
AZURE_OPENAI_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_DEPLOYMENT=nucleus-gpt-35
AZURE_OPENAI_ENDPOINT=https://openai-nucleus.openai.azure.com

# Gmail API for email sending
GMAIL_CLIENT_ID=your_gmail_client_id_here
GMAIL_CLIENT_SECRET=your_gmail_client_secret_here
GMAIL_REFRESH_TOKEN=your_gmail_refresh_token_here
GMAIL_FROM_EMAIL=your_email@gmail.com
GMAIL_FROM_NAME=Your Name

# Airtable for CRM/lead tracking
AIRTABLE_PERSONAL_ACCESS_TOKEN=your_airtable_pat_here
AIRTABLE_BASE_ID=your_airtable_base_id_here

# Rate limiting
DAILY_EMAIL_LIMIT=100
HOURLY_EMAIL_LIMIT=10 